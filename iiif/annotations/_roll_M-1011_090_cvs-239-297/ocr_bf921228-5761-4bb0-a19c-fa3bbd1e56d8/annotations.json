      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "The Navajo Indians do a great deal of blanket weaving and get a good price ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/b80a20925941d423f73b186aa634da01/41",
