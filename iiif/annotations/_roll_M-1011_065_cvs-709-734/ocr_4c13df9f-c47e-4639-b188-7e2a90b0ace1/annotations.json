      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "of the older and adult people who fight the government in every way are ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/c48a7a2f72f1c1d6f8d457ab616750a0/9",
      "@type": "oa:Annotation",
      "motivation": "sc:painting",
