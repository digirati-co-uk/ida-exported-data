      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "to the people here, we have had quite a great deal of trouble to get these ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/c48a7a2f72f1c1d6f8d457ab616750a0/6",
