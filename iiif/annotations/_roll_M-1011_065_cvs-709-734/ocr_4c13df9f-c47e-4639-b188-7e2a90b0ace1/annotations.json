      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "they were left alone, we could get them to merry legally but the influence ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/c48a7a2f72f1c1d6f8d457ab616750a0/8",
