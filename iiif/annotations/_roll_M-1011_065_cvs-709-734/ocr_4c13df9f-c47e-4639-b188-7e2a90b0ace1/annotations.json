      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "young people to properly observe the merriage customs of the State. If ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/c48a7a2f72f1c1d6f8d457ab616750a0/7",
      "@type": "oa:Annotation",
