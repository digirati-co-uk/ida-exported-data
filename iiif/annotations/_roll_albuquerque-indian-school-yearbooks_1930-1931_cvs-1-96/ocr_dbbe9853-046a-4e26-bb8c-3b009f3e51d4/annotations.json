      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "When I was a small boy, many children were going to school and I was anxious to go ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/648e46addc879b3c1fd9474e482c6d5c/5",
