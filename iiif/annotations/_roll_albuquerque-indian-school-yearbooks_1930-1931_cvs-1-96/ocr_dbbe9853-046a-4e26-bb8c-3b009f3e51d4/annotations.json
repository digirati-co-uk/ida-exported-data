      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "too for I heard that the boys and girls were learning a lot of new things in this world. ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/648e46addc879b3c1fd9474e482c6d5c/6",
