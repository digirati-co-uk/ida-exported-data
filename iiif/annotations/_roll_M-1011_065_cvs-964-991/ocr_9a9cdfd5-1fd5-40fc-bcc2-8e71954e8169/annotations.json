      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "Every year I get grape cuttings, fruit trees and such things from ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/f09d3bbf75edcbc85c44a6bc10c7ef3d/42",
