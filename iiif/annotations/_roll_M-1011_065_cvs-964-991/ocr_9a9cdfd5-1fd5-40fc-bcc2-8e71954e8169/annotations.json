      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "to fight something very unusual. ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/f09d3bbf75edcbc85c44a6bc10c7ef3d/47",
