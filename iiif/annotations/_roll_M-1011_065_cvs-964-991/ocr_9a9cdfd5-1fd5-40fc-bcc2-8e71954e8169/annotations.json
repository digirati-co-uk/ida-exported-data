      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "( b). The cut- worms usually destroy the crops in dry weather ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/f09d3bbf75edcbc85c44a6bc10c7ef3d/44",
