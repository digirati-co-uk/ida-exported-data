      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "such as we are now having. With some rain during the planting season ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/f09d3bbf75edcbc85c44a6bc10c7ef3d/45",
      "@type": "oa:Annotation",
      "motivation": "sc:painting",
