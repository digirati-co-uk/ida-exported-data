      "resource": {
        "@type": "cnt:ContentAsText",
        "chars": "something the Indians here have not attempted to any extent heretofore. ",
        "format": "text/plain"
      }
    },
    {
      "@id": "https://elucidate.dlcs-ida.org/anno/f09d3bbf75edcbc85c44a6bc10c7ef3d/41",
